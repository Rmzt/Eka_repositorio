<Page
    x:Class="D9H1.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:D9H1"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Button x:Name="truck_Button" Content="Trucks&#xD;&#xA;" HorizontalAlignment="Left" Margin="273,97,0,0" VerticalAlignment="Top" Height="171" Width="313" Click="truck_Button_Click" Background="#AAAAAAAA" FontSize="64" BorderThickness="0" ClickMode="Press"/>
        <Button x:Name="car_Button" Content="Cars&#xD;&#xA;" HorizontalAlignment="Left" Margin="691,97,0,0" VerticalAlignment="Top" Height="171" Width="313" Click="car_Button_Click" Foreground="Black" Background="#AAAAAAAA" FontSize="64" BorderThickness="0" ClickMode="Press"/>
        <TextBlock x:Name="truckTextBlock" HorizontalAlignment="Left" Margin="305,351,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Height="250" Width="250" TextAlignment="Center" FontSize="96"/>
        <TextBlock x:Name="carTextBlock" HorizontalAlignment="Left" Margin="723,351,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Height="250" Width="250" TextAlignment="Center" FontSize="96"/>

    </Grid>
</Page>
